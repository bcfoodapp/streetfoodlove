  PRIMARY KEY (`VendorID`, `AreaName`),
  CONSTRAINT `areas_ibfk_1`
    FOREIGN KEY (`VendorID`)
    REFERENCES `streetfoodlove`.`vendor` (`ID`)
    ON DELETE CASCADE
    ON UPDATE CASCADE)
ENGINE = InnoDB
DEFAULT CHARACTER SET = utf8mb4
